{
  "name": "coteline",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:supabase": "bun run scripts/test-supabase.ts",
    "test:services": "bun run scripts/test-services.ts",
    "test:all": "bun run test:supabase && bun run test:services",
    "clean": "rm -rf node_modules && bun install",
    "clean:cache": "expo start --clear",
    "clean:ios": "rm -rf ios/build",
    "clean:xcode": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "db:push": "bunx supabase db push",
    "db:reset": "bunx supabase db reset",
    "db:diff": "bunx supabase db diff",
    "db:types": "bunx supabase gen types typescript --project-id ggkdvwovpdjalqiyuelh > types/database.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.50.0",
    "expo": "~53.0.9",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-haptics": "^14.1.4",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-edge-to-edge": "^1.6.0",
    "react-native-nitro-modules": "^0.25.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-unistyles": "^3.0.0-rc.4",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.5",
    "react-test-renderer": "19.0.0",
    "supabase": "^2.24.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
